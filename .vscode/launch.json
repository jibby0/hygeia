    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "unit tests",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=pycors",
                    "--package=pycors"
                ],
                "filter": {
                    "name": "pycors",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "pycors list",
            "cargo": {
                "args": [
                    "build",
                    "--bin=pycors",
                    "--package=pycors"
                ],
                "filter": {
                    "name": "pycors",
                    "kind": "bin"
                }
            },
            "args": [
                "list",
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "pycors setup bash",
            "cargo": {
                "args": [
                    "build",
                    "--bin=pycors",
                    "--package=pycors"
                ],
                "filter": {
                    "name": "pycors",
                    "kind": "bin"
                }
            },
            "args": [
                "setup",
                "bash",
            ],
            "cwd": "${workspaceFolder}"
        },
    ]
}